{
  "name": "blih_cli",
  "registryNames": {
    "npmjs": "blih_cli",
    "github": "@greendjango/blih_cli"
  },
  "version": "1.2.0",
  "description": "Blih CLI for Linux",
  "keywords": [
    "cli",
    "prompt",
    "tty",
    "blih",
    "git",
    "command-line"
  ],
  "main": "prod/index.js",
  "engines": {
    "node": ">=14"
  },
  "bin": {
    "blih_cli": "prod/index.js",
    "bcli": "prod/index.js"
  },
  "man": "./man/json.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/GreenDjango/blih_cli.git"
  },
  "author": "Theo C.",
  "license": "MIT",
  "scripts": {
    "dev": "env BLIH_CLI_CONFIG_SKIP=true ./build/index.js",
    "build:dev": "tsc && chmod a+x build/index.js",
    "build:prod": "rm -rf build prod && npm run build:dev -s && cp -r build prod",
    "lint": "eslint src --ext .js,.ts",
    "typescript:check": "tsc --noEmit --pretty",
    "prettier:diff": "prettier --config package.json --list-different src",
    "unzip:man": "gunzip -c man/manpage.1.gz > man/manpage.1"
  },
  "dependencies": {
    "axios": "^1.6.3",
    "chalk": "^4.1.2",
    "cli-cursor": "^2.1.0",
    "cli-spinners": "^2.5.0",
    "cli-width": "^2.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.5.2",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "inquirer-fuzzy-path": "^2.3.0",
    "ora": "^5.4.1",
    "run-async": "^2.4.0",
    "strip-ansi": "^4.0.0"
  },
  "devDependencies": {
    "@types/cli-cursor": "^2.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/inquirer-autocomplete-prompt": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": true,
    "printWidth": 100
  }
}